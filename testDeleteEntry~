#!/bin/bash
curl -i -X POST -d '{}' http://localhost:5000/myCalendar/1

curl -i -H "Content-Type: application/json" -X POST -d '{"title":"Test1"}' http://localhost:5000/myCalendar/1/Entry/1

curl -i -H "Content-Type: application/json" -X POST -d '{"title":"Test1"}' http://localhost:5000/myCalendar/1/Entry/2

curl -i -X GET http://localhost:5000/myCalendar/1

curl -i -X DELETE http://localhost:5000/myCalendar/1/Entry/1

curl -i -X GET http://localhost:5000/myCalendar/1
